<Window x:Class="DbTextEditor.SaveWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Save File to Database" Height="155" Width="287" MinWidth="287" MaxWidth="287" MinHeight="155" MaxHeight="155">
    <Grid>
        <TextBox x:Name="TextBox_FileName" MaxLength="16" HorizontalAlignment="Left" Height="23" Margin="10,36,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="249"/>
        <Button x:Name="Button_Cancel" Content="Cancel" IsCancel="True" HorizontalAlignment="Left" Margin="184,84,0,0" VerticalAlignment="Top" Width="75"/>
        <Button x:Name="Button_Save" Content="Save" HorizontalAlignment="Left" Margin="91,84,0,0" VerticalAlignment="Top" Width="75" Click="Button_Save_Click"/>
        <Label Content="File name (16 symbols max)" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
    </Grid>
</Window>
