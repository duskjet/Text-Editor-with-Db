<Window x:Class="DbTextEditor.OpenWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Open File from Database" Height="312" Width="342" MinWidth="342" MaxWidth="342" MinHeight="312" MaxHeight="312">
    <Grid>
        <Button x:Name="button_Open" Content="Open" Margin="148,244,0,0" Height="20" VerticalAlignment="Top" HorizontalAlignment="Left" Width="75" Click="button_Open_Click"/>
        <Button x:Name="button_Cancel" Content="Cancel" IsCancel="True" Margin="238,244,0,0" Height="20" VerticalAlignment="Top" HorizontalAlignment="Left" Width="75"/>
        <ListBox x:Name="listBox" Height="234" VerticalAlignment="Top" Initialized="listBox_Initialized"/>

    </Grid>
</Window>
